	<script>
	   
	    var password = document.getElementById("ContentPlaceHolder1_signupPassword")
    , confirm_password = document.getElementById("confirm_password");
	    /*
        --------------------------validatePassword()-------------------------------------------------------

        This function checks two password textbox element to validate that two textboxes have same value 
        or not
       
       ----------------------------------------------------------------------------------------------------
        */
	        function validatePassword() {
	            if (password.value != confirm_password.value) {// If two values are not same
	                confirm_password.setCustomValidity("Passwords Don't Match");// Set custom error message 
	            } else {
	                confirm_password.setCustomValidity('');// Set empty error message
	            }
	        }

	        password.onchange = validatePassword; // On change password value , triggers validatePassword function 
	        confirm_password.onkeyup = validatePassword;

	 
	       
	</script>
