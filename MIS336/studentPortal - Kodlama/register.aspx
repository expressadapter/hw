    <script>
        /*
        -------------------------validateForm()----------------------------------------------------

        This function simulates form submitting, that way it triggers HTML 5 form control functions
        and check form elements. If all form elements are valid it triggers real submit button to 
        send data to server otherwise HTML 5 form controls shows error message.
          
        -------------------------------------------------------------------------------------------        
        */
        function validateForm() {
            if ($('#ContentPlaceHolder1_signupName')[0].checkValidity() && $('#ContentPlaceHolder1_signupSurname')[0].checkValidity() && $('#ContentPlaceHolder1_signupBday')[0].checkValidity() && $('#ContentPlaceHolder1_signupUniversity')[0].checkValidity() && $('#ContentPlaceHolder1_signupDepartment')[0].checkValidity() && $('#ContentPlaceHolder1_signupEmail')[0].checkValidity() && $('#ContentPlaceHolder1_signupPassword')[0].checkValidity() && $('#confirm_password')[0].checkValidity()) {
                $("#ContentPlaceHolder1_registerSubmit").trigger("click"); // Trigger real submit button
            }
        }
    </script>
	
     <script>  

